    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-object-from-entries": "^1.0.0",
    "babel-preset-minify": "^0.5.0",
